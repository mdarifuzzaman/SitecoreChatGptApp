{
  "$schema": "../../.sitecore/schemas/ModuleFile.schema.json",
  "namespace": "ChatGpt",
  "description": "This is serialization for chatgpt default items and templates",
  "items": {
	"includes": [
	     {
			"name": "template",
			"path": "/sitecore/templates/Feature/ChatGpt",
			"scope": "ItemAndDescendants",
			"allowedPushOperations": "CreateUpdateAndDelete"
		},
		{
			"name": "settings",
			"path": "/sitecore/content/Home/Settings",
			"allowedPushOperations": "CreateUpdateAndDelete"
		},
		{
			"name": "LaunchpadButton",
			"path": "/sitecore/client/Applications/Launchpad/PageSettings/Buttons/Tools/ChatGpt",
			"allowedPushOperations": "CreateUpdateAndDelete",
			"database": "core"
		}
	]
  }
}